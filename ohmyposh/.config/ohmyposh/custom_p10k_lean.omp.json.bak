{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bright": "#ffffff",
    "medium-bright": "#e2e2e2",
    "medium": "#c6c6c6",
    "medium-dim": "#474747",
    "dim-bg": "#1f1f1f",
    "darker-bg": "#131313",
    "dark-bg": "#1b1b1b",
    "segment-bg": "#222222",
    "os-bg": "p:medium-dim",
    "root-bg": "p:darker-bg",
    "path-bg": "p:segment-bg",
    "git-bg": "p:darker-bg",
    "error-fg": "#ffb4ab"
  },
  "transient_prompt": {
    "template": "\ue285\ueab6 ",
    "newline": true
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<p:bright>╭─</>",
          "foreground": "p:bright"
        },
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "",
          "template": "{{ .Icon }} ",
          "foreground": "p:medium",
          "background": "p:os-bg"
        },
        {
          "background": "p:root-bg",
          "foreground": "p:bright",
          "style": "diamond",
          "template": "<parentBackground>\ue0b0</> \uf292 ",
          "type": "root"
        },
        {
          "background": "p:path-bg",
          "foreground": "p:medium-bright",
          "trailing_diamond": "",
          "template": "<parentBackground></> 󰉖 {{ path .Path .Location }}",
          "type": "path",
          "style": "diamond"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "background": "p:git-bg",
          "foreground": "p:medium-bright",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ .HEAD }} ",
          "options": {
            "branch_icon": "",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "branch_template": "{{ trunc 25 .Branch }}"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:bright",
          "style": "plain",
          "template": "{{ if eq .Code 0 }}╰─󱐋{{ else }}╰─{{ end }}",
          "type": "text"
        },
        {
          "foreground": "p:medium",
          "foreground_templates": ["{{ if gt .Code 0 }}p:error-fg{{ end }}"],
          "options": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285\ueab6",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 4,
  "patch_pwsh_bleed": true,
  "final_space": true
}
